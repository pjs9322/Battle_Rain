package display_Set;

import java.awt.Graphics;
import java.awt.Label;
import java.awt.TextField;
import main.constant;

public class Login extends display_Set {
	private static final long serialVersionUID = 1L;
	
	private Label namelabel = new Label("아이디 : ", Label.RIGHT);
	private Label passwordLabel = new Label("패스워드 : ", Label.CENTER);
	private TextField userText = new TextField(6);
	private TextField passwordText = new TextField(6);
	passwordText.setEchoChar('*');
	
	@Override
	public void draw(Graphics g) {
		g.drawImage(toolkit.getImage(constant.L_I_Route[0]), 0, 0, this);
	}
}
