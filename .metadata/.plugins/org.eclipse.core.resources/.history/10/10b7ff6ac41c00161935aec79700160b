package display_Sub;

public class Alart_Mass extends Sub_Frame {
	private static final long serialVersionUID = 1L;

	public Alart_Mass(String massage) {
		super(null, 250, 100);

		this.setTitle("경고");
		this.Sub_Lable.setName(massage);
		
		this.remove(Sub_Text);
		this.remove(cancel_Button);

		this.ok_Button.setText("입장");
		this.ok_Button.addActionListener(actionListener);
		this.ok_Button.setActionCommand("Cancel");
	}
	
	public void Cancel() {
		this.dispose();
	}
}
