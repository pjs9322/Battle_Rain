package display_Sub;

public class Alart_Mass extends Sub_Frame {
	private static final long serialVersionUID = 1L;

	public Alart_Mass(String massage) {
		super(null, 250, 100);

		this.setTitle("规 茫扁");
		this.Sub_Lable.setName("规 力格");
		
		this.remove(Sub_Text);
		this.remove(cancel_Button);

		this.ok_Button.setText("涝厘");
		this.ok_Button.addActionListener(actionListener);
		this.ok_Button.setActionCommand("Search");
	}
}
